@model List<Subject>

<h1>Subjects</h1>
<a href="subjects/create" class="waves-effect waves-light btn">Add subject</a>
<a href="" class="waves-effect waves-light btn" style="background-color: red">Remove subject</a>
<form>
    Search: <input type="text" id="query" name="query" />
    <button type="submit" class="btn">Search</button>
</form>
<table style="color: #fff" class="striped">
    <thead>
        <tr>
        </tr>
    </thead>
    <tbody>
        @foreach(var su in Model)
        {
            <tr>
                <td>@su.Name</td>
                <td><a href="">View students </a></td>
            </tr>
            
        }
    </tbody>
</table> 